@model Webinarek.Models.WebinarViewModel

@{
    ViewBag.Title = "Display";
}
<div class="container">
    <div class="row">
        <h3>@Model.Title</h3>
    </div>
    <div class="row">
        <video style="margin-left:170px" width="800"controls="controls">
            <source src="/api/video/@Model.FileName/@Model.MimeType" type="video/mp4">
        </video>
    </div>
    <div class="row">
        <p>
            @Model.Description
        </p>
    </div>
</div>





