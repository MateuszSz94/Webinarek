@model System.Collections.Generic.List<Webinarek.Models.Entity.Webinar>

@{
    ViewBag.Title = "Index";
}

<a href="/Webinar/AddEdit/0">Dodaj</a>
<table class="table table-bordered table-responsive table-hover">
    <thead>
        <tr>
            <th>Tytuł</th>
            <th>Opis</th>
            <th>Akcje</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var webinar in Model)
        {
            <tr>
                <td>@webinar.Title</td>
                <td>@webinar.Description</td>
               
                <td>
                    <a href="/Webinar/AddEdit/@webinar.Id">Edytuj</a>
                    <a href="/Webinar/Display/@webinar.Id">Wyświetl</a>
                </td>
            </tr>
        }
    </tbody>
</table>


